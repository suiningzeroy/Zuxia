<div class = "span8">
<h3 class="strong">All Users</h3>
<table class = "table table-striped">
		<thead>
    		<tr>
	      	<th>Name</th>
	      	<th>Email</th>
    	  	<th>Id</th>
    	  	<th>Admin?</th>
    		</tr>
	  	</thead>
  		<tbody>
  		<% @allusers.each do |user|%>
			<tr>
			<td><%=user.name%></td>
			<td><%=user.email%></td>
			<td><%=user.id%></td>
    		<td><%=user.admin?%></td>
    		</tr>
    	<%end%>	
    	</tbody>
	</table>
</div>
